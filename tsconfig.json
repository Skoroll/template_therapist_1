{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2020"],
    "allowJs": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*", "src/react-app.d.ts"],
  "exclude": ["node_modules"]
}
